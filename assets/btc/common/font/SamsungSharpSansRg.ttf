	<script type="text/javascript">
	
		var pageErrorAlign = function() {
	
		var cont = $('.error-conts');
	
			var l = parseInt(($(window).width() - $(cont).width() ) / 2);
			var t = parseInt(($(window).height() - $(cont).height() ) / 2);
					
			$(cont).css({
				"top" : t + "px",
				"left" : l + "px"
			});
			$(cont).css('width','$(cont).width()');
			//$('.error-container').css('height', ch);
		};
		
		$(document).ready(function() {
			pageErrorAlign();
		});
		
		$(window).resize(function() {
			pageErrorAlign();
		});

	</script>
